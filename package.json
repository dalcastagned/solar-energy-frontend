    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4",
    "formik": "^2.2.9",
    "formik-material-ui": "^4.0.0-alpha.2",
    "formik-material-ui-lab": "^1.0.0-alpha.2",
    "history": "^5.0.1",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.4",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jest-styled-components": "^7.0.5",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^11.1.2",
    "mockdate": "^3.0.5",
    "plop": "^2.7.4",
    "prettier": "^2.4.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.4.3",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src/**/*.ts[x] --max-warnings=0",
    "test": "react-scripts test --coverage a",
    "test:without-coverage": "react-scripts test a",
    "http": "react-scripts test --coverage a && http-server coverage/lcov-report",
    "eject": "react-scripts eject",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint-staged": "lint-staged",
    "commit": "git-cz",
    "commit:all": "git add . && git-cz",
    "postinstall": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts(x)?",
      "!src/**/stories.tsx",
      "!src/styles/**/*.ts",
      "!src/utils/tests/*.ts?(x)",
      "!src/App.tsx",
      "!src/index.tsx",
      "!src/**/*.d.ts",
      "!src/models/**/*.ts",
      "!src/**/types.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/test.ts?(x)"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
